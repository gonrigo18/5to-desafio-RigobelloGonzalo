<h2 style="color:crimson;">Historic</h2>

<% if(products.length > 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
        <tr style="color: yellow;">
            <th>Title</th>
            <th>Price</th>
            <th>Thumbnail</th>
        </tr>
        <% for (const product of products) {%>
        <tr>
            <td><%=product.title%></td>
            <td><%=product.price%></td>
            <td><%=product.thumbnail%></td>
        </tr>
        <%};%>
        </table>
    </div>
<%}else{%>
    <h3 class="alert alert-danger">Product not found</h3>
<%};%>